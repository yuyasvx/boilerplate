{
  "name": "electron-and-vue",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main/main.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:main": "electron-webpack dev",
    "build": "vue-cli-service build",
    "build:main": "NODE_ENV=production webpack && cp -r ./src/assets ./dist",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "dev:main": "webpack",
    "electron": "electron dist/main/main.js",
    "clean:dist": "rm -rf ./dist",
    "clean:release": "rm -rf ./release",
    "clean:all": "npm run clean:dist & npm run clean:release",
    "build:release": "npm run clean:dist && npm run build && npm run build:main",
    "make:mac": "npm run clean:all && npm run build:release && build --mac --x64"
  },
  "dependencies": {
    "electron-store": "^3.2.0",
    "vue": "^2.6.6",
    "vue-class-component": "^6.0.0",
    "vue-meta": "^1.6.0",
    "vue-property-decorator": "^8.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-module-decorators": "^0.9.8"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-typescript": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "electron": "^5.0.0",
    "electron-builder": "^20.39.0",
    "electron-rebuild": "^1.8.4",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^3.1.1",
    "sass": "^1.17.2",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.0.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.1",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  }
}
