{
  "name": "electron-and-vue",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main/main.js",
  "engines": {
    "node": ">=12.4.0"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:main": "NODE_ENV=production webpack && cp -r ./src/assets ./dist",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "dev:main": "webpack",
    "electron": "electron dist/main/main.js",
    "clean:dist": "rm -rf ./dist",
    "clean:release": "rm -rf ./release",
    "clean:all": "npm run clean:dist & npm run clean:release",
    "build:release": "npm run clean:dist && npm run build && npm run build:main",
    "make:mac": "npm run clean:all && npm run build:release && electron-builder --mac --x64",
    "postinstall": "electron-rebuild -f -w sqlite3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "electron-store": "^5.0.0",
    "fp-ts": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.0",
    "tsyringe": "^4.0.1",
    "vue": "^2.6.6",
    "vue-class-component": "^7.0.0",
    "vue-meta": "^2.0.0",
    "vue-property-decorator": "^8.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-module-decorators": "^0.9.8"
  },
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "@types/sqlite3": "^3.1.5",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "electron": "^9.4.0",
    "electron-builder": "^22.0.0",
    "electron-rebuild": "^1.8.6",
    "eslint": "^6.0.0",
    "eslint-config-standard": "^13.0.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-node": "^9.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^4.0.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.18.2",
    "sass": "^1.17.2",
    "sass-loader": "^8.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^5.3.3",
    "type-fest": "^0.7.1",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
